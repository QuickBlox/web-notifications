// https://github.com/jshint/jshint/blob/master/examples/.jshintrc
{
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "quotmark": "single",
    "undef": true,
    "unused": "strict",

    "browser": true,

    "globals": {
        "define": false,
        "module": false,
        "console": false,
        "Notification": false
    }
}
